/* pages/wallet/wallet.wxss */
page{
    height: 100%;
    background: #EFEFEF;
}
.wellet{
    margin:20rpx 30rpx;
    text-align: center;
    color: #fff;
    padding: 20rpx 0;
    border-radius: 16rpx;
    background:linear-gradient(-90deg,rgba(61,178,50,1),rgba(106,198,60,1));
}
.wellet .title{
    font-size: 30rpx;
    padding-top: 20rpx;
}
.wellet .monney{
    padding:6rpx 0;
    font-weight: 550;
    font-size: 54rpx;
    padding-bottom: 34rpx;
}
.wellet .we_btn{
    text-align: center;
    margin: 0 auto;
    padding: 16rpx 0;
}
.we_btn .btn{
    display: inline-block;
    color: #44B534;
    font-size: 32rpx;
    font-weight: normal;
    margin: 0 40rpx;
    background: #fff;
    padding: 12rpx 66rpx;
    border-radius: 12rpx;
    font-weight: 500;
}

.page_list .cell{
    width: 100%;
    display: flex;
    align-items: center;
    padding: 26rpx 30rpx;
    background: #fff;
    margin-bottom: 10rpx;
    box-sizing: border-box;
}
.page_list .cell .right_icon{
    width: 7px;
    height: 12px;
    /* position: absolute; */
    margin-left: 10px;
}
.page_list .title{
    flex: 1;
    font-size: 32rpx;
}
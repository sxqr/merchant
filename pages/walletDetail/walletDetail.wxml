<!--pages/walletDetail/walletDetail.wxml-->
<wxs module="count">
  var money = function(val) {
    return (val / 100).toFixed(2);
  }
  module.exports.money = money;
</wxs>
<view class="we_page">
    <view class="ht_20"></view>
    <view class="page_list">
        <scroll-view scroll-y lower-threshold="100" bindscrolltolower="scrollToLower" style="height: 98vh; min-height: 0px;">
            <view class="list_cell flex" wx:for="{{dataList}}">
                <view class="list_lt">
                    <view class="item">
                        <text class="wechat">提现</text>
                    </view>
                    <view class="time">
                        {{item.createTime}}
                    </view>
                </view>
                <view class="list_rt">
                    <view class="item">
                        <text class="monney">-{{count.money(item.amount)}}</text>
                    </view>
                    <!-- <view class="item">
                        <text class="all_monney">余额：11.00</text>
                    </view> -->
                </view>
            </view>
        </scroll-view>
    </view>
</view>

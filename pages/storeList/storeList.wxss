/* pages/storeList/storeList.wxss */
page{
    background: #efefef;
    height: 100%;
}
.ht_20{
    height: 20rpx;
    background: #efefef;
}
.btm_menu{
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    background: #fff;
    font-size: 34rpx;
    font-weight: 500;
}
.list_cell{
    background: #fff;
    padding: 30rpx;
    border-bottom: 1px solid #EFEFEF;
}
.list_cell .title{
    font-size: 32rpx;
    font-weight: 500;
    padding-bottom: 12rpx;
}
.list_cell .addr{
    display: flex;
    align-items: center;
    font-size: 26rpx;
}
.list_cell .addr image{
    width: 22rpx;
    height: 26rpx;
    margin-right: 10rpx;
}
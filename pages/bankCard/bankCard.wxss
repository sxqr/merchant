/* pages/bankCard/bankCard.wxss */
page{
    height: 100%;
    background: #EFEFEF;
}
.banck_list .list_cell{
    height: 260rpx;
    margin: 20rpx 30rpx 30rpx;
    background: #4DA999;
    border-radius: 16rpx;
    color: #fff;
    padding-top: 40rpx;
}
.list_cell .banck_logo{
    align-items: center;
    margin-bottom: 20rpx;
}
.banck_logo image{
    height: 80rpx;
    width: 80rpx;
    margin-left: 40rpx;
}
.banck_logo .title{
    font-size: 36rpx;
    font-weight: 500;
    margin-left: 15rpx;
}
.list_cell .card{
    margin-left: 140rpx;
    font-size: 46rpx;
    font-weight: 500;
}
.add_btn{
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    border-radius: 10rpx;
    margin: 50rpx 60rpx 0 60rpx;
    background: #DDDDDD;
    color: #fff;
    font-size: 34rpx;
    font-weight: 500;
}


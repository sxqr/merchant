/* pages/storeDetail/storeDetail.wxss */
.custom{
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    height: 44px;
    background:linear-gradient(-90deg,rgba(61,178,50,1),rgba(106,198,60,1));
    /* background: #000; */
    background-size: 100% 100%;
    z-index: 999;
}
.custom .back_title{
    float: left;
    color: #fff;
    font-size: 36rpx;
    font-weight: 500;
    width: calc(100% - 88px);
    line-height: 44px;
    text-align: left;
    margin:0 auto;
}
.custom .iphone{
    text-align: center;
}
.custom .back{
    width: 40px;
    height: 44px;
    float: left;
    display: flex;
    align-items: center;
}
.custom .back image{
    width: 32rpx;
    height: 32rpx;
    margin-left: 30rpx;
}
.empty_custom{
    height: 44px;
    width: 100%;
}
page{
    height: 100%;
    background: #efefef;
}
.store_top{
    padding: 30rpx;
    color: #fff;
    margin-bottom: 20rpx;
    background:linear-gradient(-90deg,rgba(61,178,50,1),rgba(106,198,60,1));
}
.store_title{
    padding-top: 20rpx;
}
.store_title text{
    font-weight: 500;
    font-size: 36rpx;
}
.store_title .edit{
    float: right;
    padding: 0 10rpx 10rpx;
    margin-right: -10rpx;
}
.store_title image{
    width: 30rpx;
    height: 30rpx;
}
.store_top .addr{
    align-items: center;
    padding-top: 30rpx;
}
.store_top .addr image{
    width: 24rpx;
    height: 28rpx;
    margin-right: 10rpx;
}
.store_top .addr text{
    font-size: 26rpx;
}
.store_top .grop{
    padding-top: 60rpx;
    display: flex;
}
.store_top .grop .item{
    flex: 1;
    text-align: center;
}
.store_top .grop .item .add,.store_top .grop .item .show_img{
    width: 64rpx;
    height: 64rpx;
    display: block;
    margin: 0 auto;
}
.store_top .grop .item text{
    display: block;
    padding-top:10rpx;
    font-size: 30rpx;
}
.shopclerk{
    /* padding-left: 30rpx; */
    /* padding:30rpx 0 30rpx 30rpx; */
    background: #fff;
    /* height: 90rpx;
    line-height: 90rpx; */
    font-size: 30rpx;
    border-bottom: 1px solid #F6F6F6;
}
.shopclerk .lt{
    padding: 30rpx;
}
.shopclerk .add_member{
    color: #6AC63C;
    float: right;
    padding: 30rpx;
}
.store_list .list_cell{
    background: #fff;
    padding: 30rpx;
    border-bottom: 1px solid #F6F6F6;
    align-items: center;
}
.store_list .list_lt .phone{
    align-items: center;
    padding-top: 10rpx;
    font-size: 26rpx;
}
.list_lt .phone image{
    width: 26rpx;
    height: 26rpx;
    margin-right: 8rpx;
}
.store_list .list_lt{
    flex: 1;
}
.store_list .list_lt .title{
    font-size: 32rpx;
    font-weight: 500;
}
.list_rt .code image{
    height: 40rpx;
    width: 40rpx;
    display: block;
    margin: 0 auto;
}
.list_rt .code text{
    display: block;
    font-size: 24rpx;
    padding-top: 5rpx;
}
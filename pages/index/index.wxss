.custom{
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 44px;
  /* background-image: linear-gradient(to right, #69C63C , #40B333); */
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  z-index: 999;
}
page{
  /* min-height: calc(100% + 300px); */
}
.custom_title{
  color: #fff;
  font-size: 36rpx;
  font-weight: 500;
  width: calc(100% - 60rpx);
  line-height: 44px;
  text-align: left;
  margin:0 auto;
}
.custom .iphone{
  text-align: center;
}
.empty_custom{
  height: 44px;
  width: 100%;
}
.top_bg{
  /* background: #63C33B; */
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  padding-bottom: 160rpx;
}
.my_header{
  padding: 10rpx 30rpx 0;
  align-items: center;
  color: #fff;
}
.my_header .info{
  flex: 1;
}
.my_header .logo{
  width: 110rpx;
  height: 110rpx;
  margin-right: 20rpx;
}
.my_header .logo image{
  border-radius: 100%;
}
.info .name{
  font-size: 34rpx;
  font-weight: 500;
  padding-bottom: 5rpx;
  display: flex;
  align-items: center;
}
.info .name text{
  font-weight: 550;
}
.info .name image{
  width: 14rpx;
  height: 24rpx;
  margin-left: 20rpx;
  margin-top: 3px;
}
.info .card{
  font-size: 30rpx;
}
.model{
  margin: 0 30rpx;
  margin-top: -140rpx;
}
.model .content{
  box-sizing: border-box;
  background: #fff;
  border-radius: 12rpx;
  /* box-shadow: 0 5px 5px 0 rgba(236,236,236,1); */
  box-shadow:0px 5px 10px 0px rgba(235,235,235,1);
  min-height: 156rpx;
}
.model .title{
  text-align: center;
  padding-top: 30rpx;
  font-size: 28rpx;
}
.model .monney{
  text-align: center;
  padding:20rpx 0;
  font-weight: bold;
  font-size: 54rpx;
  color: #333;
}
.monney_btm .item{
  flex: 1;
  text-align: center;
  padding: 4rpx 0 30rpx 0;
  position: relative;
}
.monney_btm .item .title{
  display: block;
  color: #999;
  font-size: 28rpx;
  padding-bottom: 10rpx;
}
.monney_btm .item .num{
  font-size: 34rpx;
  color: #333;
  font-weight: 500;
}
.monney_btm .item:nth-child(1){
  /* border: 1px solid #FEFEFE;
  border-radius: 12rpx;
  box-shadow: 0 5px 5px 0 rgba(236,236,236,1); */
}
.monney_btm .item:nth-child(1)::before{
  content: "";
  display: inline-block;
  height: 100rpx;
  width: 1rpx;
  background: #EBEBEB;
  position: absolute;
  right: 0rpx;
  top: calc((100% - 100rpx) / 2);
}
.grid_menu{
  padding-top: 35rpx;
}
.list_cell .item{
  width: 25%;
  float: left;
  text-align: center;
  padding: 26rpx 0;
}
.list_cell .item image{
  width: 56rpx;
  height: 56rpx;
  margin: 0 auto;
}
.list_cell .item text{
  font-size: 28rpx;
  display: block;
  padding-top: 10rpx;
  font-weight: 500;
}

.write{
  position:fixed; 
  top :0px;
  left: 0px;
  right: 0px;
  bottom: 0px;
  text-align: center;
  background:rgba(0,0,0,.4);
  z-index: 99;
}
.child{
  width: 80%;
  z-index: 100;
  top: 30%;
  position: fixed;
  font-size: 30rpx;
  left: 10%;
  right: 10%;
}
.child1{
  width: 80%;
  z-index: 100;
  top: 30%;
  position: fixed;
  font-size: 30rpx;
  left: 10%;
  right: 10%;
  max-height: 400rpx;
  overflow-y: scroll;
}
.child2{
  width: 80%;
  z-index: 100;
  top: 30%;
  position: fixed;
  font-size: 30rpx;
  left: 10%;
  right: 10%;
  border-radius: 16rpx;
}
.male{
  padding: 20rpx;
  border-bottom: 1px solid #efefef;
  color: #999;
  height: 40rpx;
  line-height: 40rpx;
}
.female{
  padding: 20rpx;
  color: #999;
  height: 40rpx;
  line-height: 40rpx;
}
.choose{
  color: rgb(16, 202, 248)
}

.mt-title{
  padding: 40rpx 0 20rpx 0;
  padding-bottom: 5rpx;
  font-size: 34rpx;
  font-weight: 500;
}
.content{
  font-size: 28rpx;
  color: #979797;
}
.accredit_btn{
  display: flex;
  align-items: center;
  margin-top: 70rpx;
}
.btn_no{
  width: 50%;
  background: #EDEDED;
  color: #57BF6A;
  font-size: 32rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom-left-radius: 16rpx;
}
.child2 .title{
  padding: 40rpx 0 30rpx 0;
  font-size: 34rpx;
}
.btn_yes{
  width: 50% !important;
  background: #57BF6A;
  color: #fff;
  font-weight: 500;
  font-size: 32rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 0;
  padding: 0;
  margin: 0;
  border-bottom-right-radius: 16rpx;
}